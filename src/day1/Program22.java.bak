package day1;

import java.util.Scanner;

class Program22
{
	//to find the factorial of first n natural numbers using loop.
	static int factorial(int a)
	{
		if (a > 1)
		{
		 return a*factorial(a-1);
		}
		else
		{
			return 1;
		}
	}
	public static void main(String[] args) 
	{
